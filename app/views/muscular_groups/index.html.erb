<p id="notice"><%= notice %></p>

<h1>Muscular Groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @muscular_groups.each do |muscular_group| %>
      <tr>
        <td><%= muscular_group.name %></td>
        <td><%= link_to 'Show', muscular_group %></td>
        <td><%= link_to 'Edit', edit_muscular_group_path(muscular_group) %></td>
        <td><%= link_to 'Destroy', muscular_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Muscular Group', new_muscular_group_path %>
